    "plotly.js-dist": "^1.52.3",
    "register-service-worker": "^1.7.1",
    "tailwindcss": "^1.2.0",
    "vue": "^2.6.11",
    "vue-axios": "^2.1.5",
    "vue-modal-dialogs": "^3.0.0",
    "vue-router": "^3.1.6",
    "vue-spinner": "^1.0.3",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-eslint": "^4.2.3",
    "@vue/cli-plugin-pwa": "^4.2.3",
    "@vue/cli-plugin-router": "^4.2.3",
